<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net5.0</TargetFramework>
        
        <Company>Happytravel.com</Company>
        <Authors>Oleg Konovalov, Hasmik Harutyunyan, Kirill Taran</Authors>
        <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Version>1.0.0</Version>
        <PackageReleaseNotes>Added MultiLanguage</PackageReleaseNotes>
        <Description>MultiLanguage class library</Description>
        <PackageProjectUrl>https://github.com/happy-travel/multi-language</PackageProjectUrl>
        <RepositoryURL>https://github.com/happy-travel/multi-language</RepositoryURL>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    </ItemGroup>

</Project>
